<!doctype html>
<html>
<head>
    <title>Burda Linetoday</title>
    <link href="/css/bootstrap-4/css/bootstrap.min.css" rel="stylesheet">
    <link href="/css/home.css" rel="stylesheet">

    <!-- Load Google Fonts -->
    <link href="https://fonts.googleapis.com/css?family=Varela+Round" rel="stylesheet">

    <!-- load bootstrap css -->
    <!-- <link rel="stylesheet" href="//netdna.bootstrapcdn.com/bootstrap/3.0.2/css/bootstrap.min.css"> -->

    <!-- load fontawesome -->
    <link rel="stylesheet" href="//netdna.bootstrapcdn.com/font-awesome/4.0.3/css/font-awesome.min.css">
</head>
<body>
    <header id="top" class="header">
      <nav class="navbar navbar-dark bg-inverse">
        <div class="container">
          <a class="navbar-brand logo" href="/">LINE TODAY</a>
          <ul class="nav navbar-nav">
            <li class="nav-item active">
              <a class="nav-link" href="/">Home <span class="sr-only">(current)</span></a>
            </li>
            <li class="nav-item">
              <a class="nav-link" href="/admin">Admin</a>
            </li>
            <li class="nav-item">
              <a class="nav-link" href="/lisa_linegenerate">Lisaguru</a>
            </li>
            <li class="nav-item">
              <a class="nav-link" href="/hello_linegenerate">Hello!</a>
            </li>
            <li class="nav-item">
              <a class="nav-link" href="/api/logout">logout </a>
            </li>
          </ul>
        </div>
      </nav>
      <div class="clearfix"></div>
    </header>
    <div class="main">
        <table class="table table-condensed">
        <tr>
            <td> ID </td>
            <td> Title </td>
            <td> Upload Time </td>
            <td> XML Viewer </td>
        </tr>
        <% for(var i=0; i< log.length;i++){ %>
        <tr>
            <td> <%= log[i]._id %> </td>
            <td> <%= log[i].title %> </td>
            <td> <%= log[i].uploadDate %> </td>
            <td> <a href="/xmlViewer/<%=log[i]._id%>"> xml </a> </td>
        </tr>
        <% } %>
        </table>

    </div>
    <footer class="footer navbar-fixed-bottom">
      <div class="container">
        <div class="row">
          <div class="col-md-12 text-center">
            <p>&copy; 2016 Burda Digital</p>
          </div>
        </div>
      </div>
    </footer>
</body>

</html>

